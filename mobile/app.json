{
  "expo": {
    "name": "Cash Book",
    "slug": "cash-book",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "cash-book",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "image": "./image/splash.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cashbook.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./image/icon.png"
      },
      "package": "com.cashbook.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router", "expo-secure-store", "expo-font", "expo-asset"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
